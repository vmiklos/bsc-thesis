.PS
copy "sequence.pic";

actor(Actor);
pobject(Center);
pobject(Sensor);
pobject(Terminal);
step();

cmessage(Actor,Center,"c:Center");
active(Center);
step();

cmessage(Actor,Sensor,"s:Sensor");
active(Sensor);
step();

cmessage(Actor,Terminal,"t:Terminal");
active(Terminal);
step();

message(Sensor,Center,"register()");
step();

active(Actor);
message(Center,Actor,"notify(s)");
message(Actor,Center,"accept(s)");
inactive(Actor);
step();

message(Terminal,Center,"subscribe()");
step();

active(Actor);
message(Center,Actor,"notify(t)");
message(Actor,Center,"accept(t)");
inactive(Actor);
step();

inactive(Terminal);
step();

inactive(Sensor);
step();

inactive(Center);
step();
.PE

# vim: ft=pic
