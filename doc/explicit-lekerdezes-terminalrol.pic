.PS
copy "sequence.pic";

actor(Actor);
pobject(Center);
pobject(Sensor);
pobject(Terminal);
step();

cmessage(Actor,Center,":Center");
active(Center);
step();

cmessage(Actor,Sensor,":Sensor");
active(Sensor);
step();

cmessage(Actor,Terminal,":Terminal");
active(Terminal);
step();

message(Sensor,Center,"register()");
step();

message(Terminal,Center,"subscribe()");
step();

message(Terminal,Center,"lookup()");
step();

message(Terminal,Sensor,"query()");
step();

inactive(Terminal);
step();

inactive(Sensor);
step();

inactive(Center);
step();
.PE

# vim: ft=pic
